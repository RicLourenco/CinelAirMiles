@model List<CinelAirMiles.Common.Entities.Complaint>

@{
    ViewData["Title"] = "ComplaintsList";
    Layout = "_LayoutMyAccount";
}

<!--? My Account Area Start-->
<section class="blog_area section-padding" style="margin-top:-100px;">
    <h1>Complaints List</h1>
    <div class="container">
        <br />
        <br />
        <div class="row">
            <div class="col-lg-12 mb-8 mb-lg-0">
                <div class="blog_left_sidebar">                    
                    <br />
                    <ejs-grid id="complaintsGrid" allowSorting="true" allowPaging="true" allowGrouping="true" allowSelection="false" dataSource="Model">
                    </ejs-grid>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- My Account Area End -->
